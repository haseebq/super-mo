          },
          "easing": "easeInOut"
        },
        {
          "time": 0.3,
          "bones": {
            "leg.l": { "rotation": -0.5 },
            "leg.r": { "rotation": 0.5 }
          },
          "easing": "easeInOut"
        },
        {
          "time": 0.6,
          "bones": {
            "leg.l": { "rotation": 0.5 },
            "leg.r": { "rotation": -0.5 }
          },
          "easing": "easeInOut"
        }
      ]
    }
